<form method="post" action="{{ path('app_visite_delete', {'id': visite.id}) }}"
      onsubmit="return confirm('Action irreversible, continuer ??');"  style="display: inline">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ visite.id) }}">
    <button class="btn btn-xs btn-circle btn-danger btn-outline"><i class="fa fa-trash"></i> </button>
</form>
