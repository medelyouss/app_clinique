{% extends 'base.html.twig' %}

{% block title %}Visite{% endblock %}

{% block body %}
    <h1>Visite</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ visite.id }}</td>
            </tr>
            <tr>
                <th>Motif</th>
                <td>{{ visite.motif }}</td>
            </tr>
            <tr>
                <th>Interrogatoire</th>
                <td>{{ visite.interrogatoire }}</td>
            </tr>
            <tr>
                <th>Examen</th>
                <td>{{ visite.examen }}</td>
            </tr>
            <tr>
                <th>Conclusion</th>
                <td>{{ visite.conclusion }}</td>
            </tr>
            <tr>
                <th>DateVisiteAt</th>
                <td>{{ visite.dateVisiteAt ? visite.dateVisiteAt|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_visite_index') }}">back to list</a>

    <a href="{{ path('app_visite_edit', {'id': visite.id}) }}">edit</a>

    {{ include('visite/_delete_form.html.twig') }}
{% endblock %}
