{% extends 'admin/base.html.twig' %}

{% block title %}Quizz Category{% endblock %}

{% block body %}
    <h1>Quizz Categorie</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ quizz_category.id }}</td>
            </tr>
            <tr>
                <th>Designation</th>
                <td>{{ quizz_category.designation }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_quizzcategory_index') }}"><< Retour</a>

    <a href="{{ path('admin_quizzcategory_edit', {'id': quizz_category.id}) }}">Modifier</a>

    <span class="pull-right">{{ include('quizz_category/_delete_form.html.twig') }}</span>
{% endblock %}
