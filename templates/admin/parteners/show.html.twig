{% extends 'admin/base.html.twig' %}

{% block title %}Parteners{% endblock %}

{% block body %}
    <h1>Parteners</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ partener.id }}</td>
            </tr>
            <tr>
                <th>Designation</th>
                <td>{{ partener.designation }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ partener.url }}</td>
            </tr>
            <tr>
                <th>IsActif</th>
                <td>{{ partener.isActif ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ partener.image }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ partener.updatedAt ? partener.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_parteners_index') }}">back to list</a>

    <a href="{{ path('admin_parteners_edit', {'id': partener.id}) }}">edit</a>

    {{ include('admin/parteners/_delete_form.html.twig') }}
{% endblock %}
